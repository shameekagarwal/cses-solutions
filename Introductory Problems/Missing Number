#include<bits/stdc++.h>
using namespace std;
#define ll long long
 
int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);
	int n; cin >> n;
	vector<bool>a(n + 1, 0);
	for (int i = 0; i < n - 1; i++)
	{
		int x; cin >> x;
		a[x] = 1;
	}
	for (int i = 1; i <= n; i++)
	{
		if (!a[i])
		{
			cout << i << "\n";
			return 0;
		}
	}
	return 0;
}