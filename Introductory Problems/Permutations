#include<bits/stdc++.h>
using namespace std;
#define ll long long
 
int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);
	int n; cin >> n;
	if (n == 3 || n == 2)
	{
		cout << "NO SOLUTION";
		return 0;
	}
	for (int i = 2; i <= n; i += 2)
		cout << i << " ";
	for (int i = 1; i <= n; i += 2)
		cout << i << " ";
	return 0;
}