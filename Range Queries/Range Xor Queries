#include<bits/stdc++.h>
using namespace std;
#define sz 10010
#define MOD 1000000007
#define ll long long
 
int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);
	int n, q; cin >> n >> q;
	vector<int>a(n), pre(n + 1, 0);
	for (int i = 0; i < n; ++i)
	{
		cin >> a[i];
		pre[i + 1] = pre[i] ^ a[i];
	}
	while (q--)
	{
		int L, R; cin >> L >> R;
		cout << (pre[R]^pre[L - 1]) << "\n";
	}
	return 0;
}