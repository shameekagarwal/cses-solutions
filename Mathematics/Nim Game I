#include<bits/stdc++.h>
using namespace std;
#define ll long long
 
int main()
{
	ios::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);
 
	int t; cin >> t; while (t--) {
		int n; cin >> n; vector<int>a(n);
		for (auto &it : a)
			cin >> it;
		int sm = 0;
		for (auto it : a)
			sm ^= it;
		cout << (sm ? "first" : "second") << "\n";
	}
	return 0;
}